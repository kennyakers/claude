{
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1"
  },
  "permissions": {
    "allow": [
      "WebSearch",
      "WebFetch",
      "Bash(cat:*)",
      "Bash(date:*)",
      "Bash(du:*)",
      "Bash(echo:*)",
      "Bash(ffprobe:*)",
      "Bash(file:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(head:*)",
      "Bash(ls:*)",
      "Bash(mkdir:*)",
      "Bash(sort:*)",
      "Bash(source:*)",
      "Bash(tail:*)",
      "Bash(test:*)",
      "Bash(tr:*)",
      "Bash(tree:*)",
      "Bash(wc:*)",
      "Bash(git:*)",
      "Bash(python*:*)",
      "Bash(COQUI_TOS_AGREED=1 python:*)",
      "Bash(pnpm build:*)",
      "Bash(pnpm exec prettier:*)",
      "Bash(pnpm install:*)",
      "Bash(pnpm lint:*)",
      "Bash(pnpm list:*)",
      "Bash(pnpm test:*)",
      "Bash(pnpm tsc:*)",
      "Bash(pnpm *typecheck:*)",
      "Bash(pnpm --filter *eslint:*)",
      "Bash(npm view:*)",
      "Bash(npx tsc:*)",
      "Bash(npx get-shit-done-cc:*)",
      "Bash(uv run python:*)",
      "Bash(uv sync:*)",
      "Bash(pip index:*)",
      "Bash(brew list:*)",
      "Bash(xcrun simctl launch:*)",
      "Bash(idb_companion:*)",
      "mcp__ios-simulator__ui_view",
      "mcp__ios-simulator__ui_describe_all",
      "mcp__ios-simulator__ui_tap",
      "mcp__ios-simulator__screenshot",
      "mcp__ios-simulator__get_booted_sim_id",
      "mcp__context7__resolve-library-id",
      "Skill(gsd:*)",
      "Skill(superpowers:*)",
      "Skill(update-agent-knowledge)",
      "Skill(pr-review-toolkit:review-pr)",
      "Bash(pnpm typecheck:*)",
      "Bash(uv run:*)"
    ],
    "deny": [
      "Bash(git push *--force*)",
      "Bash(git push *-f*)",
      "Bash(git reset --hard*)",
      "Bash(git clean -f*)",
      "Bash(git checkout -- .*)",
      "Bash(git branch -D*)",
      "Bash(git branch *--delete --force*)",
      "Bash(git rebase*)",
      "Bash(git merge --abort*)",
      "Bash(git reflog expire*)",
      "Bash(git gc --prune*)",
      "Bash(chmod *)",
      "Bash(chown *)",
      "Bash(sudo *)",
      "Bash(kill *)",
      "Bash(killall *)",
      "Bash(pkill *)",
      "Bash(pnpm remove *)",
      "Bash(pnpm prune*)",
      "Bash(pnpm store prune*)",
      "Bash(npm uninstall *)",
      "Bash(npm cache clean*)",
      "Bash(pip uninstall *)",
      "Bash(pip install --force-reinstall*)",
      "Bash(uv pip uninstall *)",
      "Bash(brew uninstall *)",
      "Bash(brew remove *)",
      "Bash(brew cleanup --prune-all*)"
    ],
    "defaultMode": "default"
  },
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "$HOME/.claude/hooks/gsd-check-update.sh"
          }
        ]
      }
    ],
    "Stop": [],
    "PostToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node \"/Users/kakers/.claude/hooks/gsd-context-monitor.js\""
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "$HOME/.claude/hooks/statusline.sh"
  },
  "enabledPlugins": {
    "superpowers@superpowers-marketplace": true,
    "pr-review-toolkit@claude-plugins-official": true,
    "frontend-design@claude-plugins-official": true,
    "ralph-wiggum@claude-plugins-official": true,
    "swift-lsp@claude-plugins-official": true,
    "clangd-lsp@claude-plugins-official": true
  },
  "alwaysThinkingEnabled": true,
  "attribution": {
    "commit": "",
    "pr": ""
  }
}
